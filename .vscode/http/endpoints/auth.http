@authUrl = {{baseUrl}}/auth

### register as user
POST {{authUrl}}/register/user

{
  "email": "user@email.com",
  "password": "Password1"
}

### register as partner
POST {{authUrl}}/register/partner

{
  "email": "partner@email.com",
  "password": "Password1",
  "phoneNumber": "800500300",
  "nip": "1234563218"
}

### login as user
POST {{authUrl}}/login

{
  "email": "user@email.com",
  "role": "User",
  "password": "Password1",
  "rememberMe": false
}

### login as partner
POST {{authUrl}}/login

{
  "email": "partner@email.com",
  "role": "Partner",
  "password": "Password1",
  "rememberMe": false
}

### logout
# ðŸ”’ only for authenticated users or partners
POST {{authUrl}}/logout
